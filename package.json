{
  "name": "nulledge-conference",
  "version": "1.0.0",
  "description": "nullEDGE - Free ServiceNow Conference Website",
  "author": "nullEDGE Team",
  "license": "ISC",
  "type": "module",
  "engines": {
    "node": ">=20.x.x"
  },
  "scripts": {
    "clean": "rimraf dist src/_includes/css src/_includes/scripts",
    "clean:og": "rimraf src/assets/og-images",
    "dev:11ty": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "build:11ty": "cross-env ELEVENTY_ENV=production eleventy",
    "build:og": "cross-env FORCE_OG_REGEN=true npm run build:11ty",
    "start": "npm run dev:11ty",
    "build": "npm run clean && npm run build:11ty"
  },
  "keywords": [
    "servicenow",
    "conference",
    "nulledge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jjnulledge/redux.git"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-fetch": "^5.1.0",
    "@11ty/eleventy-img": "^6.0.4",
    "@11ty/eleventy-plugin-webc": "^0.11.2",
    "@11ty/is-land": "^4.0.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@toycode/markdown-it-class": "^1.2.4",
    "autoprefixer": "^10.4.22",
    "colorjs.io": "^0.5.2",
    "cross-env": "^10.1.0",
    "cssnano": "^7.1.2",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "esbuild": "^0.27.0",
    "fast-glob": "^3.3.3",
    "html-minifier-terser": "^7.2.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "markdown-it-attrs": "^4.3.1",
    "markdown-it-link-attributes": "^4.0.1",
    "netlify-plugin-cache": "^1.0.3",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "postcss-import-ext-glob": "^2.1.1",
    "postcss-js": "^5.0.1",
    "prettier-plugin-jinja-template": "^2.1.0",
    "rimraf": "^6.1.0",
    "sharp": "^0.34.5",
    "sharp-ico": "^0.1.5",
    "slugify": "^1.6.6",
    "svgo": "^4.0.0"
  }
}
